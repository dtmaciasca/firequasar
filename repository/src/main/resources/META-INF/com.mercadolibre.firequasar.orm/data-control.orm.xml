<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm
        http://xmlns.jcp.org/xml/ns/persistence/orm_2_1.xsd" version="2.1">
    <package>com.mercadolibre.firequasar.model</package>
    <mapped-superclass class="DataControl" access="FIELD">
        <attributes>
            <basic name="fechaInsert">
                <column name="fecha_insert" insertable="false" updatable="false"/>
            </basic>
            <basic name="fechaUpdate">
                <column name="fecha_update" insertable="false" updatable="false" />
            </basic>
            <basic name="fechaDelete">
                <column name="fecha_delete" insertable="false"/>
            </basic>
            <basic name="deleted">
                <column name="deleted"/>
            </basic>
            <basic name="version">
                <column name="version" insertable="false" updatable="false"/>
            </basic>
        </attributes>
    </mapped-superclass>
</entity-mappings>